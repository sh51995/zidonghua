<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="util.mysql.entity.mapping.userDeviceMapper">
 
     <select id="getUserDeviceByuserid" parameterType="String" 
         resultType="util.mysql.entity.model.UserDevice">
         select * from user_device where user_id=#{user_id}
     </select>
   
 </mapper>